    "number1=input(\"enter number:\")\n",
    "number=int(number1)\n",
    "if number>1:\n",
    "    for i in range(2,int(number/2) + 1):\n",
    "        if(number%i)==0:\n",
    "            print(number,\"number is not prime\")\n",
    "        else:\n",
    "            print(number,\"number is prime\")\n",
    "else:\n",
    "    print(number,\"is not a prime\")"
